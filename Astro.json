.tab {
  position: relative;
  margin: 10 10px;
}

.tab-link {
  display: inline-block;
  padding: 10px;
  text-align: center;
  color: white;
  text-decoration: none;
  border-bottom: 2px solid transparent;
  transition: border-bottom 0.3s ease-in-out;
}

.tab-link:hover {
  border-bottom: 2px solid blue;
  border-radius: 15px;
}

.tab-link.active {
  border-bottom: 2px solid var(--primary);
  border-radius: 15px;
}

.tab-link.active:before {
  content: "";
  position: absolute;
  bottom: -9px;
  left: 0;
  width: 100%;
  height: 2px;
  background-color: var(--primary);
  border-radius: 15px;
  
  
  
  
}

.tab-link.active[data-direction="left"]:before {
  animation: move-bar-left 0.3s ease-in-out;
}

.tab-link.active[data-direction="right"]:before {
  animation: move-bar-right 0.3s ease-in-out;
}

@keyframes move-bar-left {
  0% {
    transform: translateX(100%);
  }
  100% {
    transform: translateX(0);
  }
}

@keyframes move-bar-right {
  0% {
    transform: translateX(-100%);
  }
  100% {
    transform: translateX(0);
  }
}

.tab-content {
  margin-top: 20px;
  
}

.tab-content .tab {
  display: none;
}

.tab-content .active {
  display: block;
}

