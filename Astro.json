 <script>
const slider = document.getElementById("sliderAuto");
slider.innerHTML += slider.innerHTML;

let scrollStep = 1;
let interval = null;
let userScrolling = false;
let resumeTimeout;

function autoScroll() {
  if (userScrolling) return;
  slider.scrollLeft += scrollStep;
  if (slider.scrollLeft >= slider.scrollWidth / 2) slider.scrollLeft = 0;
}

function startAutoScroll() {
  if (!interval) interval = setInterval(autoScroll, 20);
}

function stopAutoScroll() {
  if (interval) { clearInterval(interval); interval = null; }
}

function scheduleResume() {
  clearTimeout(resumeTimeout);
  resumeTimeout = setTimeout(() => { userScrolling = false; startAutoScroll(); }, 1500);
}

slider.addEventListener("mousedown", () => { userScrolling = true; stopAutoScroll(); });
slider.addEventListener("mouseup", scheduleResume);
slider.addEventListener("mouseleave", () => { if (userScrolling) scheduleResume(); });

slider.addEventListener("touchstart", () => { userScrolling = true; stopAutoScroll(); }, { passive: true });
slider.addEventListener("touchend", scheduleResume);

slider.addEventListener("wheel", () => { userScrolling = true; stopAutoScroll(); scheduleResume(); }, { passive: true });
slider.addEventListener("scroll", () => {
  if (slider.scrollLeft >= slider.scrollWidth / 2) slider.scrollLeft = 0;
});

startAutoScroll();
</script>  
<script>
(function () {

  const contenedor = document.getElementById("ecomendados");
  if (!contenedor) return;

  const vistos = JSON.parse(localStorage.getItem("vistos")) || [];
  const favoritos = JSON.parse(localStorage.getItem("favoritos")) || [];

  const intereses = new Set();

  [...vistos, ...favoritos].forEach(item => {
    if (item.genero) intereses.add(item.genero);
    if (item.tags) item.tags.split(",").forEach(t => intereses.add(t));
  });

  if (intereses.size === 0) return;

  const posters = document.querySelectorAll(".postersarp");

  let recomendados = [];

  posters.forEach(p => {
    const genero = p.dataset.genero;
    const tags = p.dataset.tags || "";

    intereses.forEach(i => {
      if (genero === i || tags.includes(i)) {
        recomendados.push(p);
      }
    });
  });

  recomendados = [...new Set(recomendados)].slice(0, 12);

  recomendados.forEach(p => {
    const clone = p.cloneNode(true);
    contenedor.appendChild(clone);
  });

})();
</script>  
   
   
   
    
    

    
