// Para El Silider Sarp
const slides = document.querySelectorAll('.SliderSarp-slide');
const progress = document.querySelector('.SliderSarp-progress');
let currentSlide = 0;
let slideInterval = setInterval(nextSlide, 4000);

function nextSlide() {
  slides[currentSlide].classList.remove('active');
  currentSlide = (currentSlide + 1) % slides.length;
  slides[currentSlide].classList.add('active');
  updateProgress();
}

function manualSlide(n) {
  clearInterval(slideInterval);
  slides[currentSlide].classList.remove('active');
  currentSlide = (n + slides.length) % slides.length;
  slides[currentSlide].classList.add('active');
  updateProgress();
  slideInterval = setInterval(nextSlide, 4000);
}

function updateProgress() {
  const percent = ((currentSlide + 1) / slides.length) * 100;
  progress.style.width = percent + '%';
}

for (let i = 0; i < slides.length; i++) {
  const button = slides[i].querySelector('.SliderSarp-button');
  const slideIndex = i; // Guardar el √≠ndice del slide en una variable
  button.addEventListener('click', function() {
    window.location.href = 'pagina' + (slideIndex + 1) + '.html';
  });
}

document.addEventListener('keydown', function(e) {
  if (e.code === 'ArrowLeft') {
    manualSlide(currentSlide - 1);
  } else if (e.code === 'ArrowRight') {
    manualSlide(currentSlide + 1);
  }
});
     
     

// Funcion de las pesta√±as
   var tabLinks = document.querySelectorAll(".tab-link");
var tabContents = document.querySelectorAll(".tab-content .tab");
var bar = document.querySelector(".tab-bar");

function activateTab(tab) {
  var id = tab.getAttribute("href").substring(1);
  
  tabContents.forEach(function(content) {
    content.classList.remove("active");
  });

  document.getElementById(id).classList.add("active");

  tabLinks.forEach(function(link) {
    link.classList.remove("active");
  });

  tab.classList.add("active");

  var activeTab = document.querySelector(".tab-link.active");
  var activeTabRect = activeTab.getBoundingClientRect();
  var barRect = bar.getBoundingClientRect();

  var start = activeTabRect.left - barRect.left;
  var end = start + activeTabRect.width;

  bar.style.left = start + "px";
  bar.style.width = activeTabRect.width + "px";

  requestAnimationFrame(function() {
    bar.style.transform = "translateX(" + (end - start) + "px)";
  });
}

tabLinks.forEach(function(link) {
  link.addEventListener("click", function(e) {
    e.preventDefault();
    activateTab(this);
  });
});

// Javascript del buscador
var posters = document.querySelectorAll('.postersarp');
var searchInput = document.getElementById('search-input');
var searchButton = document.getElementById('search-button');
var resultados = document.getElementById('resultados');
var mensajeBusqueda = document.getElementById('mensaje-busqueda');
var mensajeTexto = document.getElementById('mensaje-texto');
var cerrarMensaje = document.getElementById('cerrar-mensaje');

// Registro para almacenar los nombres de los p√≥sters ya agregados a los resultados
var p√≥stersAgregados = {};

function buscarPosters() {
  // limpiar resultados anteriores
  resultados.innerHTML = '';

  var searchTerm = searchInput.value.toLowerCase();

  // si el t√©rmino de b√∫squeda est√° vac√≠o, ocultar el mensaje y salir de la funci√≥n
  if (searchTerm.trim() === '') {
    mensajeBusqueda.style.display = 'none';
    return;
  }

  // buscar posters que coincidan con el t√©rmino de b√∫squeda
  var foundResults = false;
  p√≥stersAgregados = {}; // Reiniciar el registro de p√≥sters cada vez que se realiza una b√∫squeda
  for (var i = 0; i < posters.length; i++) {
    var poster = posters[i];
    var altText = poster.querySelector('img').getAttribute('alt').toLowerCase();
    if (altText.includes(searchTerm)) {
      var posterName = altText.trim(); // Obtener el nombre del p√≥ster sin espacios en blanco
      // Verificar si el p√≥ster ya ha sido agregado a los resultados
      if (!p√≥stersAgregados[posterName]) {
        foundResults = true;
        p√≥stersAgregados[posterName] = true; // Agregar el nombre del p√≥ster al registro
        // clonar el poster y agregarlo a los resultados
        var clonedPoster = poster.cloneNode(true);
        resultados.appendChild(clonedPoster);
      }
    }
  }

  // mostrar mensaje si no se encontraron resultados
  if (!foundResults) {
    mensajeTexto.innerHTML = 'No se encontraron resultados para la b√∫squeda "' + searchTerm + '"';
    mensajeBusqueda.style.display = 'block';
  } else {
    mensajeBusqueda.style.display = 'none';
  }
}

function ocultarMensaje() {
  mensajeBusqueda.style.display = 'none';
}

// buscar posters al presionar el bot√≥n de b√∫squeda
searchButton.addEventListener('click', function() {
  buscarPosters();
  ocultarMensaje();
});

// buscar posters al presionar la tecla Enter
searchInput.addEventListener('keydown', function(event) {
  if (event.key === 'Enter') {
    buscarPosters();
    ocultarMensaje();
  }
});

// cerrar mensaje al presionar el bot√≥n "cerrar"
cerrarMensaje.addEventListener('click', function() {
  ocultarMensaje();
});

// mostrar mensaje al cargar la p√°gina
mostrarMensaje();

// mostrar mensaje al hacer clic en la barra de b√∫squeda
searchInput.addEventListener('click', function() {
  mostrarMensaje();
});

// ocultar mensaje al hacer clic en el bot√≥n de b√∫squeda
searchButton.addEventListener('click', function() {
  ocultarMensaje();
});

// ocultar mensaje cuando se borra el contenido del campo de b√∫squeda
searchInput.addEventListener('input', function() {
  if (searchInput.value.trim() === '') {
    ocultarMensaje();
  }
});

// funci√≥n para mostrar el mensaje
function mostrarMensaje() {
  var mensaje = document.getElementById('mensajese');
  mensaje.classList.add('show');
}

// funci√≥n para ocultar el mensaje
function ocultarMensaje() {
  var mensaje = document.getElementById('mensajese');
  mensaje.classList.remove('show');
}




</script>


  <script>
    // Al cargar la p√°gina, verifica si el p√≥ster est√° marcado como favorito
    window.addEventListener('DOMContentLoaded', function() {
      var corazones = document.querySelectorAll('.corazon');

      for (var i = 0; i < corazones.length; i++) {
        var corazon = corazones[i];
        var id = corazon.parentNode.getAttribute('id');
        var imagen = localStorage.getItem(id + 'Imagen');

        if (imagen) {
          corazon.classList.add('favorito');
        }
      }
    });

    function marcarFavorito(id, enlaceId, corazon) {
      corazon.classList.toggle('favorito');

      var estaMarcado = corazon.classList.contains('favorito');

      if (estaMarcado) {
        localStorage.setItem(id + 'Imagen', corazon.parentNode.querySelector('img').src);
        localStorage.setItem(id + 'Enlace', enlaceId);
        document.getElementById('mensajeYTA').innerText = 'Se ha agregado a favoritos';
      } else {
        localStorage.removeItem(id + 'Imagen');
        localStorage.removeItem(id + 'Enlace');
        document.getElementById('mensajeYTA').innerText = 'Se ha eliminado de favoritos';
      }

      document.getElementById('mensajeYTA').style.display = 'block';
      setTimeout(function() {
        document.getElementById('mensajeYTA').style.display = 'none';
      }, 3000);
    }
  </script>

     
     
