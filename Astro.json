document.querySelectorAll('.SliderSarp-slide').forEach(slide => {
  const video = slide.querySelector('.trailer-video');

  if (!video) return;

  slide.addEventListener('mouseenter', () => {
    if (window.matchMedia('(hover: hover)').matches) {
      video.currentTime = 0;
      video.play();
    }
  });

  slide.addEventListener('mouseleave', () => {
    video.pause();
    video.currentTime = 0;
  });
});
</script>
<script>
(function () {

  const contenedor = document.getElementById("recomendados");
  if (!contenedor) return;

  const vistos = JSON.parse(localStorage.getItem("vistos")) || [];
  const favoritos = JSON.parse(localStorage.getItem("favoritos")) || [];

  const intereses = new Set();

  [...vistos, ...favoritos].forEach(item => {
    if (item.genero) intereses.add(item.genero);
    if (item.tags) item.tags.split(",").forEach(t => intereses.add(t));
  });

  if (intereses.size === 0) return;

  const posters = document.querySelectorAll(".postersarp");

  let recomendados = [];

  posters.forEach(p => {
    const genero = p.dataset.genero;
    const tags = p.dataset.tags || "";

    intereses.forEach(i => {
      if (genero === i || tags.includes(i)) {
        recomendados.push(p);
      }
    });
  });

  recomendados = [...new Set(recomendados)].slice(0, 12);

  recomendados.forEach(p => {
    const clone = p.cloneNode(true);
    contenedor.appendChild(clone);
  });

})();
</script>
