});

function toggleMenu() {
  var menu = document.getElementById("menu");


  if (menu.classList.contains("closed")) {
    menu.classList.remove("closed");
    menu.classList.add("open");
    content.classList.add("open-menu");
  } else {
    menu.classList.remove("open");
    menu.classList.add("closed");
    content.classList.remove("open-menu");
  }
}

function generateCreator() {
  var menu = document.getElementById("menu");

  var creatorSection = document.createElement("div");
  creatorSection.classList.add("creator-section");

  var creatorImage = document.createElement("img");
  creatorImage.classList.add("creator-image");
  creatorImage.src = "https://i.ibb.co/svmPFXZH/images-12.jpg";
  creatorImage.alt = "Creador";
  creatorSection.appendChild(creatorImage);

  var creatorTitle = document.createElement("div");
  creatorTitle.classList.add("creator-title");
  creatorTitle.textContent = "AstroMovie";
  creatorSection.appendChild(creatorTitle);

  var contactButton = document.createElement("a");
  contactButton.classList.add("contact-button");
  contactButton.textContent = "CHAT";
  contactButton.href = "https://www.instagram.com/astromovie2025?igsh=MTkwcWdnMG95ODA3aA==";
  creatorSection.appendChild(contactButton);
  
  var groupButton = document.createElement("a");
  groupButton.classList.add("group-button");
  groupButton.textContent = "DMCA";
  groupButton.href = "https://www.amd.com/es/legal/notices/dmca.html#:~:text=La%20Ley%20sobre%20Derechos%20de,celeridad%20el%20contenido%20infractor%20tras";
  creatorSection.appendChild(groupButton);
  
  

  menu.insertBefore(creatorSection, menu.firstChild);
  
}

// Cerrar la ventana del men√∫ al hacer clic fuera de ella
window.addEventListener("click", function(event) {
  var menu = document.getElementById("menu");
  var target = event.target;
  var isMenuIcon = target.id === "menu-icon";
  var isInsideMenu = menu.contains(target);
  var isMenuOpen = menu.classList.contains("open");

  if (isMenuOpen && !isMenuIcon && !isInsideMenu) {
    toggleMenu();
  }
});

const scrollMenu = document.querySelector('.scrollmenu');

scrollMenu.addEventListener('touchstart', () => {
  scrollMenu.classList.add('is-paused'); // Agregar la clase 'is-paused' al tocar
});

scrollMenu.addEventListener('touchend', () => {
  scrollMenu.classList.remove('is-paused'); // Quitar la clase 'is-paused' al dejar de tocar
});

  window.addEventListener('DOMContentLoaded', function() {
  var seleccionesContainer = document.getElementById('seleccionesContainer');
  var startX;
  var scrollLeft;

  seleccionesContainer.addEventListener('mousedown', function(e) {
    startX = e.pageX - seleccionesContainer.offsetLeft;
    scrollLeft = seleccionesContainer.scrollLeft;
  });

  seleccionesContainer.addEventListener('mousemove', function(e) {
    if (!startX) return;
    e.preventDefault();
    var x = e.pageX - seleccionesContainer.offsetLeft;
    var walk = (x - startX) * 3; // Ajusta la velocidad del deslizamiento
    seleccionesContainer.scrollLeft = scrollLeft - walk;
  });

  seleccionesContainer.addEventListener('mouseup', function() {
    startX = null;
  });

  seleccionesContainer.addEventListener('mouseleave', function() {
    startX = null;
  });

  function agregarSeleccion(posterId) {
    var posterSeleccionado = document.getElementById(posterId);

    // Comprobar si el poster seleccionado ya est√° presente en las selecciones
    var seleccionExistente = seleccionesContainer.querySelector('#' + posterId);

    // Si ya existe, moverlo al principio del contenedor
    if (seleccionExistente) {
      seleccionesContainer.prepend(seleccionExistente);
      return; // Detener la ejecuci√≥n de la funci√≥n
    }

    var posterClonado = posterSeleccionado.cloneNode(true);
    posterClonado.classList.add('active');
    posterClonado.querySelectorAll('.corazon').forEach(corazon => corazon.style.display = 'none');

    // Establecer el ancho deseado en el c√≥digo JavaScript
    posterClonado.style.width = '75px'; // Reemplaza '200px' con el ancho deseado en p√≠xeles

    // Agregar el poster clonado al principio del contenedor
    seleccionesContainer.prepend(posterClonado);

    // Limitar el n√∫mero de selecciones a 80
    var seleccionActual = seleccionesContainer.getElementsByClassName('posterSeleccionado');
    if (seleccionActual.length > 20) {
      seleccionesContainer.removeChild(seleccionActual[20]);
    }

    // Guardar la selecci√≥n en el almacenamiento local del navegador
    localStorage.setItem('seleccion_' + posterId, posterId);

    // Mostrar el texto "Continuar Viendo"
    var tituloContainer = document.getElementById('tituloContainer');
    tituloContainer.style.display = 'block';
  }

  // Recuperar las selecciones guardadas al cargar la p√°gina
  for (var i = 0; i < localStorage.length; i++) {
    var key = localStorage.key(i);
    if (key.startsWith('seleccion_')) {
      var posterId = localStorage.getItem(key);
      agregarSeleccion(posterId);
    }
  }

  window.agregarSeleccion = agregarSeleccion;
});


  </script>
  
<script>
// Funci√≥n para mostrar los elementos seg√∫n el g√©nero seleccionado
function mostrarPorGenero(genero) {
// Obt√©n todos los elementos con la clase "postersarp" y el atributo "data-genero" igual al g√©nero seleccionado
  const elementosFiltrados = Array.from(document.querySelectorAll(`.postersarp[data-genero="${genero}"]`));

  // Verifica si el g√©nero es v√°lido y si existen elementos filtrados para mostrar
  if (genero && elementosFiltrados.length > 0) {
    // Obt√©n el div correspondiente al g√©nero utilizando el ID
    const divGenero = document.getElementById(genero);

    // Verifica si el div existe
    if (divGenero) {
      // Clonar los elementos filtrados y luego insertar los clones dentro del div correspondiente al ID del g√©nero
      elementosFiltrados.forEach(elemento => {
        const clone = elemento.cloneNode(true); // true indica que tambi√©n se deben clonar los elementos hijos
        divGenero.appendChild(clone);
      });
    }
  }
}

