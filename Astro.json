.scrollmenu-container {
  max-width: 600px; /* Puedes ajustar este valor seg√∫n tus necesidades */
  overflow-x: scroll;
  scroll-snap-type: x mandatory;
}

.scrollmenu {
  white-space: nowrap;
  scroll-behavior: smooth;
  width: max-content;
  scroll-padding: 0 10px;
  animation: scroll 200s linear infinite;
  animation-play-state: running; /* A√±adimos la propiedad animation-play-state inicialmente en 'running' */
}

.scrollmenu.is-paused {
  animation-play-state: paused; /* A√±adimos la clase 'is-paused' para detener la animaci√≥n */
}

.scrollmenu > * {
  scroll-snap-align: start;
}

@keyframes scroll {
  0% {
    transform: translateX(0);
  }
  40% {
    transform: translateX(-100%);
  }
  60% {
    transform: translateX(-100%);
  }
  100% {
    transform: translateX(0);
  }
}



