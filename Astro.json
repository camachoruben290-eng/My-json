<script>
document.querySelectorAll('.auto-scroll').forEach(row => {
  let scrollAmount = 0;
  let speed = 0.5; // velocidad (menor = m√°s suave)
  let paused = false;

  // Scroll autom√°tico
  function autoScroll() {
    if (!paused) {
      row.scrollLeft += speed;

      // Cuando llega al final, regresa al inicio
      if (row.scrollLeft + row.clientWidth >= row.scrollWidth - 5) {
        row.scrollLeft = 0;
      }
    }
    requestAnimationFrame(autoScroll);
  }

  autoScroll();

  // Pausar al tocar / pasar mouse
  row.addEventListener('mouseenter', () => paused = true);
  row.addEventListener('mouseleave', () => paused = false);
  row.addEventListener('touchstart', () => paused = true);
  row.addEventListener('touchend', () => paused = false);
});
</script>
<script>
const carousel = document.getElementById("nuevoCarousel");

let speed = 0.3; // üî• VELOCIDAD (m√°s alto = m√°s r√°pido)
let autoScroll;

function startAutoScroll() {
  autoScroll = setInterval(() => {
    carousel.scrollLeft += speed;

    // Reinicia cuando llega al final
    if (
      carousel.scrollLeft + carousel.clientWidth >=
      carousel.scrollWidth - 5
    ) {
      carousel.scrollLeft = 0;
    }
  }, 10);
}

// Pausa al tocar o pasar el dedo
carousel.addEventListener("touchstart", () => clearInterval(autoScroll));
carousel.addEventListener("mouseenter", () => clearInterval(autoScroll));

// Reanuda
carousel.addEventListener("touchend", startAutoScroll);
carousel.addEventListener("mouseleave", startAutoScroll);

startAutoScroll();
</script>
