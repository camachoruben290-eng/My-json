  <style>
    :root {
      --bg: #141414;
      --text: #fff;
      --accent: #ff0303;
      --card-bg: #242424;
      --size: 48vmin;
      --height: var(--size);
      --width: calc(0.65 * var(--size));
      --font-size: calc(0.74 * var(--size));
      --gap: calc(0.15 * var(--size));
    }

    * {
      box-sizing: border-box;
      margin: 0;
      padding: 0;
    }

    body {
      font-family: 'Roboto', sans-serif;
      background: #000;
      color: var(--text);
      overflow-x: hidden;
      line-height: 1.5;
    }

    .top-picks {
      max-width: 100%;
      padding: 0;
      background: #000;
    }

    .top-picks__title {
      font-family: 'Roboto', sans-serif;
      color: var(--text);
      font-size: clamp(1.8rem, 5vmin, 2.8rem);
      font-weight: 900;
      margin: -2.5rem 0.5rem 1.5rem;
      text-shadow: 0 2px 8px rgba(0, 0, 0, 0.7);
      position: relative;
    }

    .top-picks__title:before {
      content: '';
      position: absolute;
      bottom: -5px;
      left: 0;
      width: 50px;
      height: 3px;
      background: var(--accent);
      border-radius: 2px;
      animation: titleGlow 3s infinite;
    }

    @keyframes titleGlow {
      0% { width: 50px; }
      50% { width: 70px; }
      100% { width: 50px; }
    }

    .top-picks__track {
      height: var(--height);
      margin: 0;
      padding: 0 var(--gap);
      display: flex;
      list-style-type: none;
      max-width: 100%;
      overflow-x: auto;
      overflow-y: visible;
      counter-reset: index;
      -ms-scroll-snap-type: x mandatory;
      scroll-snap-type: x mandatory;
      -webkit-overflow-scrolling: touch;
      scrollbar-width: none;
      position: relative;
    }

    .top-picks__track::-webkit-scrollbar {
      display: none;
    }

    .top-picks__item {
      overflow: visible;
      width: calc(var(--width) + var(--gap) / 2);
      flex: 0 0 calc(var(--width) + var(--gap) / 2);
      position: relative;
      list-style: none;
      counter-increment: index;
      padding-left: calc(var(--gap) / 2);
      scroll-snap-align: center;
      transition: transform 0.3s ease;
    }

    .top-picks__item:hover {
      transform: translateY(-10px);
    }

    .top-picks__item .item__card {
      width: var(--width);
      position: absolute;
      right: 0;
      top: 0;
      bottom: 0;
      border-radius: 2.5vmin;
      background: var(--card-bg);
      overflow: hidden;
      box-shadow: 0 6px 20px rgba(0, 0, 0, 0.5);
      transition: transform 0.3s ease, box-shadow 0.3s ease;
      border: 2px solid transparent;
    }

    .top-picks__item:hover .item__card {
      transform: scale(1.08);
      box-shadow: 0 10px 30px rgba(229, 9, 20, 0.3);
      border-color: rgba(229, 9, 20, 0.5);
    }

    .top-picks__item .item__card img {
      position: absolute;
      height: 100%;
      width: 100%;
      object-fit: cover;
      transition: filter 0.2s ease;
      filter: saturate(50%);
    }

    .top-picks__item:hover .item__card img {
      filter: saturate(100%) brightness(1.1);
    }

    .top-picks__item .item__card span {
      text-decoration: none;
      position: absolute;
      top: 2rem;
      right: 2rem;
      max-width: 50%;
      text-align: right;
      color: var(--text);
      font-family: 'Roboto', sans-serif;
      font-weight: 900;
      font-size: clamp(1.1rem, 2vmin, 1.6rem);
      text-shadow: 0.5vmin 0 3vmin var(--bg);
      z-index: 2;
    }

    .top-picks__item:after {
      content: counter(index);
      position: absolute;
      color: var(--bg);
      font-family: 'Roboto', sans-serif;
      font-size: var(--font-size);
      line-height: 0.7;
      bottom: 0;
      font-weight: 900;
      letter-spacing: -5vmin;
      left: 0;
      -webkit-text-stroke: 0.6vmin #fff;
      text-shadow: 1.2vmin 0 5vmin var(--bg);
      transform: translate(-14%, 0);
      z-index: 1;
    }

    /* Responsive Design */
    @media (max-width: 1024px) {
      .top-picks__item {
        width: calc(1.1 * var(--width) + var(--gap) / 2);
        flex: 0 0 calc(1.1 * var(--width) + var(--gap) / 2);
      }
    }

    @media (max-width: 768px) {
      .top-picks__item {
        width: calc(1.3 * var(--width) + var(--gap) / 2);
        flex: 0 0 calc(1.3 * var(--width) + var(--gap) / 2);
      }

      .top-picks__item .item__card span {
        font-size: clamp(0.9rem, 1.5vmin, 1.3rem);
      }
    }

    @media (max-width: 480px) {
      .top-picks__item {
        width: calc(1.6 * var(--width) + var(--gap) / 2);
        flex: 0 0 calc(1.6 * var(--width) + var(--gap) / 2);
      }
    }
  </style>
